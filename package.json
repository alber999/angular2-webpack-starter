{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "repository": {
    "type": "git",
    "url": "git@github.com:alber999/angular2-webpack-starter.git"
  },
  "scripts": {
    "i18n-extract": "ng-xi18n",
    "start": "gulp i18n-xlf2ts && webpack-dev-server --inline --progress --port 8080",
    "start:prod": "http-server dist -p 8080 -c-1",
    "test": "gulp i18n-xlf2ts && karma start",
    "test-coverage": "gulp i18n-xlf2ts && karma start karma-coverage.conf.js",
    "build": "gulp i18n-xlf2ts && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.8",
    "@angular/compiler": "~2.4.8",
    "@angular/core": "~2.4.8",
    "@angular/forms": "~2.4.8",
    "@angular/http": "~2.4.8",
    "@angular/platform-browser": "~2.4.8",
    "@angular/platform-browser-dynamic": "~2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@angular/router": "~3.4.1",
    "core-js": "^2.4.1",
    "rxjs": "5.0.2",
    "zone.js": "^0.7.4",
    "compression-webpack-plugin": "^0.3.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.53",
    "angular2-template-loader": "^0.6.0",
    "async": "^2.1.4",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-modify-file": "^0.1.0",
    "gulp-remove-empty-lines": "0.0.8",
    "gulp-rename": "^1.2.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "ignore-loader": "^0.1.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "pretty-data": "^0.40.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.6",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.0.0"
  }
}
