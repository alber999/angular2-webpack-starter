{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "repository": {
    "type": "git",
    "url": "git@github.com:alber999/angular2-webpack-starter.git"
  },
  "scripts": {
    "i18n:extract": "ng-xi18n",
    "i18n:create": "gulp i18n-init --languages",
    "i18n:update": "gulp i18n-update",
    "i18n:compile": "gulp i18n-xlf2ts",
    "start": "npm run i18n:compile && webpack-dev-server --inline --progress --port 8080",
    "start:prod": "http-server dist -p 8080 -c-1",
    "test": "karma start",
    "test:coverage": "karma start karma-coverage.conf.js",
    "build": "npm run i18n:compile && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.8",
    "@angular/compiler": "~2.4.8",
    "@angular/core": "~2.4.8",
    "@angular/forms": "~2.4.8",
    "@angular/http": "~2.4.8",
    "@angular/platform-browser": "~2.4.8",
    "@angular/platform-browser-dynamic": "~2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@angular/router": "~3.4.1",
    "@ngtools/webpack": "^1.2.10",
    "compression-webpack-plugin": "^0.3.2",
    "core-js": "^2.4.1",
    "rxjs": "5.0.2",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "angular2-template-loader": "^0.6.2",
    "async": "^2.1.5",
    "awesome-typescript-loader": "^3.0.6",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-modify-file": "^0.1.0",
    "gulp-remove-empty-lines": "0.0.8",
    "gulp-rename": "^1.2.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "ignore-loader": "^0.1.2",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "pretty-data": "^0.40.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^2.6.1"
  }
}
